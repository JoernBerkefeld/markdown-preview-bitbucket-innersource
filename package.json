    "description": "Replaces the Markdown preview with Bitbucket Innersource styles.",
    "version": "2.0.2",
    "publisher": "joernberkefeld",
    "license": "MIT",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#eee",
        "theme": "light"
    },
    "keywords": [
        "Markdown",
        "Bitbucket",
        "vscode",
        "vscode-extension",
        "innersource"
    ],
    "contributes": {
