    },
    "bugs": {
        "url": "https://github.com/JoernBerkefeld/markdown-preview-bitbucket-innersource/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JoernBerkefeld/markdown-preview-bitbucket-innersource.git",
        "homepage": "https://github.com/JoernBerkefeld/markdown-preview-bitbucket-innersource"
    }
